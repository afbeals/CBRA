{
  "name": "generator-cbra-component",
  "version": "1.0.0",
  "description": "cbra component generator",
  "author": {
    "name": "Allan Beals-Gibson",
    "url": "http://www.allanbg.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "react",
    "component",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-xo": "^0.25.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.2.0",
    "jest": "^29.3.1",
    "lint-staged": "^8.1.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.jsx": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "airbnb-base",
      "eslint:recommended"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "indent": 0,
      "no-tabs": 0,
      "class-methods-use-this": 0,
      "one-var": 0
    }
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "CDK/generator-cbra-component",
  "private": true
}
